
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Listname</title>
</head>
<body>


    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">First</th>
                <th scope="col">Last</th>

            </tr>
        </thead>
        <tbody>

            @foreach (var x in ViewBag.Alluser)
            {
                <tr>

                    <th scope="row">@x.id</th>
                    <td>@x.username</td>
                    <td>@x.usersurname</td>

                </tr>
            }

        </tbody>
    </table>






</body>
</html>
